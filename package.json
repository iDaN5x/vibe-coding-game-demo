{
  "name": "legal-rag",
  "version": "1.0.0",
  "description": "Browser 3D third-person tennis shooter (Kadosh)",
  "type": "module",
  "scripts": {
    "start": "npx --yes serve . -p 3000",
    "dev": "live-server . --port=3000 --open=/ --watch=src,index.html,public",
